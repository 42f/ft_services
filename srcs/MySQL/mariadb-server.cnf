[mysqld]
user=root
port=3306
bind-address = 0.0.0.0
socket = /run/mysqld/mysqld.sock
datadir=/var/lib/mysql
log-bin=/var/lib/mysql/mysql-bin
skip-networking=false

[mysqld_safe]
port = 3306
socket = /run/mysqld/mysqld.sock
datadir=/var/lib/mysql

[mysql]
user=root
port=3306
socket = /run/mysqld/mysqld.sock
